{
  "name": "simp-simpsetup",
  "version": "0.2.0",
  "author": "SIMP Team",
  "summary": "Special-purpose profile for TESTING that pre-configures a freshly-bootstrapped SIMP server using `puppet apply`",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/simp-packer",
  "project_page": "https://github.com/simp/simp-packer",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [ "simp", "profile", "post-install" ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 6.4.0 < 8.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.6.0 < 8.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
